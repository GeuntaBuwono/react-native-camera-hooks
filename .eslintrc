{
    "extends": ["react-native-prettier", "eslint:recommended"],
    "plugins": [
      "react-native",
      "react-hooks"
    ],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    },
    "env": {"es6": true, "jest": false},
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "impliedStrict": true,
        "globalReturn": false,
        "jsx": true
      }
    },
    "globals": { "fetch": false, "alert": false, "describe": false, "expect": false, "it": false, "XMLHttpRequest": false, "test": false },
    "rules": {
      "no-var": "warn",
      "no-console": "warn",
      "strict": "error",
      "comma-dangle": "off",
      "no-alert": "warn",
      "react-hooks/rules-of-hooks": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
}
